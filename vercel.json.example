{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "crons": [
    {
      "path": "/api/cron/cleanup",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/send-emails",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/daily-report",
      "schedule": "0 9 * * 1-5"
    }
  ]
}
